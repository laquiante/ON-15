auto lo
iface lo inet loopback
        address 192.168.0.202/32

auto mgmt
iface mgmt
        address 127.0.0.1/8
        address ::1/128
        vrf-table auto

auto eth0
iface eth0 inet dhcp
        vrf mgmt

